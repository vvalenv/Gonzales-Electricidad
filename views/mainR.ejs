<div class="div_mainL">
    <div class="login_container">
        <p class="title_login">Registrate</p>
        <form action="" class="form_login">
            <label for="nombreU">Usuario</label>
            <input type="text" placeholder="Ingresa un nombre de usuario" id="nombreU" class="input_name">
            <label for="email">Email</label>
            <input type="mail" placeholder="Ingresa un email" id="email" class="input_email">
            <label for="contra">Contraseña</label>
            <input type="password" placeholder="Ingresa una contraseña" id="contra" class="input_pass">
            <label for="contra2">Confirma contraseña</label>
            <input type="password" placeholder="Vuelve a ingresar la contraseña" id="contra2" class="input_pass">
            <a href="/Login" class="link_register">¿Ya estas registrado? Inicia sesion</a>
            <button type="submit" class="button_login">Ingresar</button>
        </form>
    </div>
</div>